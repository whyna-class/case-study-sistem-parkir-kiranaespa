generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}


enum jenisKendaraan {
  roda2
  roda4
}

model Parkir {
  id               Int            @id @default(autoincrement())
  platNomor        String
  jenisKendaraan   jenisKendaraan
  durasi           Int
  total            Int
  createdAt        DateTime       @default(now())
  updatedAt        DateTime       @updatedAt
}
